    "manifest_version": 2,
    "name": "chell",
    "description": "Chrome shell",
    "version": "0.0",
    
    "browser_action": {
        "default_icon": "i/icon.png",
        "default_title": "chell",
        "default_popup": "body.html"
    },
    
    "permissions": [
        "tabs",
        "bookmarks",
        "clipboardWrite"
    ]
    
    
}